<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.restaurantea3.MainActivity"
        tools:layout="@layout/activity_main">
        <action
            android:id="@+id/action_FirstFragment_to_LoginClient"
            app:destination="@id/LoginClient" />
    </fragment>

    <fragment
        android:id="@+id/LoginClient"
        android:name="com.example.restaurantea3.Login_cliente"
        tools:layout="@layout/login_cliente">
        <action
            android:id="@+id/action_LoginClient_to_CadastroCliente"
            app:destination="@id/CadastroCliente" />
    </fragment>

    <fragment
        android:id="@+id/CadastroCliente"
        android:name="com.example.restaurantea3.Cadastro_cliente"
        tools:layout="@layout/cadastro_cliente">
    </fragment>
</navigation>